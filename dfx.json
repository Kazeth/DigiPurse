{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/app.mo",
      "declarations": {
        "node_compatibility": true,
        "output": "frontend/src/declarations/backend"
      }
    },
    "Document_backend": {
      "main": "backend/Document/Document.mo",
      "type": "motoko",
      "declarations": {
        "output": "frontend/src/declarations/Document_backend"
      }
    },
    "Registry_backend": {
      "main": "backend/Registry.mo",
      "type": "motoko",
      "declarations": {
        "node_compatibility": false,
        "output": "frontend/src/declarations/Registry_backend"
      } 
    },
    "User_backend": {
      "main": "backend/User.mo",
      "type": "motoko",
      "dependencies": [
        "Registry_backend"
      ],
      "declarations" : {
        "output": "frontend/src/declarations/User_backend"
      }
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "type": "assets",
      "source": [
        "frontend/dist/"
      ],
      "declarations": {
        "output": "frontend/src/declarations/frontend"
      }
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "type": "custom",
      "specified_id": "rdmx6-jaaaa-aaaaa-aaadq-cai",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "declarations": {
        "output": "frontend/src/declarations/internet_identity"
      }
    }
  },
  "output_env_file": ".env",
  "defaults": {
    "build": {
      "packtool": "npx ic-mops sources"
    }
  }
}